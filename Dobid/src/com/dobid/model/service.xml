<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd" >
<sqlMap namespace="service">
	
	<typeAlias alias="service_answerDTO" type="com.dobid.beans.Service_answerDTO" />
	<typeAlias alias="service_reportDTO" type="com.dobid.beans.Service_reportDTO" />

	<insert id="serviceOno" parameterClass="service_answerDTO">
		insert into service_answer values
			(#member_id#, #title#, #categori#, #contents#, #image_path#, to_date(#upload_date#,'YYYY-MM-DD:HH24:MI:SS'))
	</insert>
	
	<insert id="serviceReport" parameterClass="service_reportDTO">
		insert into service_report values
			(#member_id#, #title#, #contents#, #image_path#,to_date(#upload_date#,'YYYY-MM-DD:HH24:MI:SS'))
	</insert>

</sqlMap>
